<h1>User info</h1>
<!-- form_with部分 -->
<%= form_with model: @users, url:user_path, local:true do |f| %>
  <h4>name</h4>
  <%= f.text_field :name %>
  
  
  <%= attachment_image_tag @users, :profile_image, :fill, 60, 60, fallback: "no_image.jpg"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.attachment_field :profile_image, placeholder: "プロフィール画像" %>
  
  <h4>introduction</h4>
  <%= f.text_area :introduction %>
  <%= f.submit 'Update User' %>
<% end %>